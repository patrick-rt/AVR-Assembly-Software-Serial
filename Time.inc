;Using r31, r30, r29

;16MHz
.if FREQUENCY == 16000000
sek:
; Generated by delay loop calculator
; at http://www.bretmulvey.com/avrdelay.html
;
; Delay 16 000 000 cycles
; 1s at 16.0 MHz
    ldi  r31, 82
    ldi  r30, 43
    ldi  r29, 0
L1: dec  r29
    brne L1
    dec  r30
    brne L1
    dec  r31
    brne L1
    lpm
    nop
	ret
.endif

